 사용할 필드 이름입니다 (예: user, users)\r\nexport function createAsyncHandler(type, key) {\r\n    // 성공, 실패에 대한 액션 타입 문자열을 준비합니다.\r\n    const SUCCESS = `${type}_SUCCESS`;\r\n    const ERROR = `${type}_ERROR`;\r\n\r\n    // 함수를 새로 만들어서\r\n    function handler(state, action) {\r\n        switch (action.type) {\r\n        case type:\r\n            return {\r\n            ...state,\r\n            [key]: loadingState\r\n            };\r\n        case SUCCESS:\r\n            return {\r\n            ...state,\r\n            [key]: success(action.data)\r\n            };\r\n        case ERROR:\r\n            return {\r\n            ...state,\r\n            [key]: error(action.error)\r\n            };\r\n        default:\r\n            return state;\r\n        }\r\n    }\r\n    // 반환합니다\r\n    return handler;\r\n    \r\n}\r\n",["105","106"],"D:\\git_repository\\my-app\\src\\api.js",[],"D:\\git_repository\\my-app\\src\\AppRouter.js",[],"D:\\git_repository\\my-app\\src\\Home.js",[],"D:\\git_repository\\my-app\\src\\About.js",[],"D:\\git_repository\\my-app\\src\\Profile.js",[],"D:\\git_repository\\my-app\\src\\Profiles.js",[],"D:\\git_repository\\my-app\\src\\HistroySample.js",[],"D:\\git_repository\\my-app\\src\\WithRouterSample.js",[],{"ruleId":"107","replacedBy":"108"},{"ruleId":"109","replacedBy":"110"},{"ruleId":"107","replacedBy":"111"},{"ruleId":"109","replacedBy":"112"},{"ruleId":"113","severity":1,"message":"114","line":6,"column":7,"nodeType":"115","messageId":"116","endLine":6,"endColumn":13},{"ruleId":"113","severity":1,"message":"117","line":11,"column":7,"nodeType":"115","messageId":"116","endLine":11,"endColumn":14},{"ruleId":"113","severity":1,"message":"118","line":27,"column":7,"nodeType":"115","messageId":"116","endLine":27,"endColumn":19},{"ruleId":"107","replacedBy":"119"},{"ruleId":"109","replacedBy":"120"},"no-native-reassign",["121"],"no-negated-in-lhs",["122"],["121"],["122"],"no-unused-vars","'fadeIn' is assigned a value but never used.","Identifier","unusedVar